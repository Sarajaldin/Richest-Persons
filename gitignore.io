        "outputId": "3e4b1a7c-9b30-462a-a0a8-070f53a9fbda"
      },
      "outputs": [
        {
          "data": {
            "text/plain": [
              "3.3541019662496847"
            ]
          },
          "execution_count": 127,
          "metadata": {},
          "output_type": "execute_result"
        }
      ],
      "source": [
        "t = (dbar-0)/sd\n",
